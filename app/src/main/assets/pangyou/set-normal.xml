<plain>
    <header>
        <text background="#5c9afd" height="144">
            <limg src="img/normal/left_w_arrow.png" attr="60" margin="42,0,12,0" click="document.finish()"/>
            <lp font="42,#ffffff" click="document.finish()">返回</lp>
            <cp font="51,#ffffff">通用</cp>
        </text>
    </header>
    <body background="#f2f2f2">
        <text height="150" margin="0,3,0,0" click="document.open('pangyou/set-privacy.xml')" background="#ffffff">
            <lp margin="39,0,0,0" font="45,#171718">清楚缓存</lp>
            <rimg attr="42" margin="0,0,39,0" src="img/user/右箭头.png"/>
        </text>
        <text id="image" height="150" margin="0,3,0,0" click="
document.isImageSave = !document.isImageSave;
document.$('image').get('rimg').set({src:document.isImageSave?'img/user/开关-开.png':'img/user/开关-关.png'});" background="#ffffff">
            <lp margin="39,0,0,0" font="45,#171718">照片</lp>
            <rimg attr="150" margin="0,0,42,0" src="img/user/开关-关.png"/>
        </text>
        <text id="video" height="150" margin="0,3,0,0" click="
document.isVideoSave = !document.isVideoSave;
document.$('video').get('rimg').set({src:document.isVideoSave?'img/user/开关-开.png':'img/user/开关-关.png'});
" background="#ffffff">
            <lp margin="39,0,0,0" font="45,#171718">视频</lp>
            <rimg attr="150" margin="0,0,42,0" src="img/user/开关-关.png"/>
        </text>
    </body>
    <footer>
        <script>
            document.isVideoSave = false;
            document.isImageSave = false;
        </script>
    </footer>
</plain>